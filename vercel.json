{
  "version": 2,
  "builds": [
    { "src": "api/enquiry.py", "use": "@vercel/python" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/api/enquiry$", "dest": "api/enquiry.py" },
    { "src": "^/timeline$", "dest": "public/timeline.html" },
    { "src": "^/$", "dest": "public/index.html" }
  ]
}
