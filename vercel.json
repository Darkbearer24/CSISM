{
  "version": 2,
  "cleanUrls": true,
  "functions": {
    "api/enquiry.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/timeline", "destination": "/public/timeline.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}

